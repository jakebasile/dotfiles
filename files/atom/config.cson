"*":
  "autocomplete-plus":
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  "bracket-colorizer":
    alternate_different: true
  "bracket-matcher":
    alwaysSkipClosingPairs: true
    autocompleteBrackets: false
    autocompleteCharacters: [
      "()"
      "[]"
      "{}"
      "\"\""
      "“”"
      "‘’"
      "«»"
    ]
  chlorine:
    "refresh-mode": "full"
  core:
    autoHideMenuBar: true
    closeDeletedFileTabs: true
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".flowconfig"
        ".hgrc"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    destroyEmptyPanes: true
    disabledPackages: [
      "deprecation-cop"
      "autocomplete-snippets"
      "lisp-paredit"
      "proto-repl-charts"
      "proto-repl"
      "linter"
      "linter-ui-default"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      ".mongo*"
      "target*"
    ]
    openEmptyEditorOnStart: true
    packagesWithKeymapsDisabled: [
      "git-blame"
    ]
    projectHome: "/Users/jakebasile/Code"
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "nord-atom-syntax"
    ]
    uriHandlerRegistration: "always"
  editor:
    atomicSoftTabs: false
    autoIndentOnPaste: false
    fontFamily: "IBM Plex Mono"
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
  "exception-reporting":
    userId: "0dacece5-6c28-aa5c-7949-423cff26188c"
  "git-blame":
    colorCommitAuthors: true
    columnWidth: 211
    dateFormatString: "YYYY-MM"
    showLastNames: false
  "git-diff": {}
  github:
    remoteFetchProtocol: "ssh"
  "line-ending-selector":
    defaultLineEnding: "LF"
  "linter-ui-default":
    showPanel: true
    showStatusBar: false
  "linter-write-good":
    severityLevel: "Warning"
  "lisp-paredit":
    indentationForms: [
      "try"
      "catch"
      "finally"
      "/^let/"
      "are"
      "/^def/"
      "fn"
      "cond"
      "condp"
      "/^if.*/"
      "/.*\\/for/"
      "for"
      "for-all"
      "/^when.*/"
      "testing"
      "doseq"
      "dotimes"
      "ns"
      "routes"
      "GET"
      "POST"
      "PUT"
      "DELETE"
      "extend-protocol"
      "loop"
      "do"
      "case"
      "with-bindings"
      "checking"
      "with-open"
    ]
    strict: false
  "markdown-preview": {}
  "nord-atom-syntax":
    accessibility:
      commentContrast: 20
  "nord-atom-ui": {}
  "one-dark-ui":
    fontSize: 14
    tabCloseButton: "Left"
    tabSizing: "Maximum"
  organized: {}
  parinfer:
    "file-extensions": [
      ".boot"
      ".cl"
      ".clj"
      ".cljc"
      ".cljs"
      ".edn"
      ".el"
      ".lfe"
      ".lisp"
      ".lsp"
      ".rkt"
      ".scm"
    ]
  "proto-repl":
    autoPrettyPrint: true
    bootPath: "/usr/local/bin/boot"
    displayExecutedCodeInRepl: false
    displayHelpText: false
    enableCompletions: false
    leinPath: "/usr/local/bin/lein"
    refreshBeforeRunningSingleTest: false
    refreshBeforeRunningTestFile: false
    refreshOnReplStart: false
  "terminal-plus":
    core: {}
  "tool-bar": {}
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  "vim-mode-plus":
    blackholeRegisteredOperators: [
      "change*"
      "substitute*"
    ]
    useClipboardAsDefaultRegister: false
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".ansi.text":
  editor:
    autoIndent: true
    preferredLineLength: 9
    scrollPastEnd: false
    softWrap: false
    softWrapAtPreferredLineLength: false
".clojure.source":
  editor:
    autoIndent: true
    autoIndentOnPaste: false
    nonWordCharacters: "()\"':,;~@#$%^&{}[]`"
".hack.html.text":
  editor:
    preferredLineLength: 9
".hackfragment.source":
  editor:
    preferredLineLength: 9
".hg-commit.text":
  editor:
    preferredLineLength: 9
".hover.reason.source.type":
  editor:
    autoIndent: true
    preferredLineLength: 110
".reason.source":
  editor:
    preferredLineLength: 110
".rust.source":
  editor:
    tabLength: 2
